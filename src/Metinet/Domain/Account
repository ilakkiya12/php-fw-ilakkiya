<?php

class Account
{

    private $id;
    private $email;
    private $password;

    /**
     * Account constructor.
     * @param $id
     * @param $Email
     * @param $password
     */
    public function __construct(int $id, string $email,string $password)
    {
        $this->id = $id;
        $this->email = $email;
        $this->password = $password;
    }

    /**
     * @return int
     */
    public function getId(): int
    {
        return $this->id;
    }

    /**
     * @return string
     */
    public function getEmail(): string
    {
        return $this->email;
    }

    /**
     * @return string
     */
    public function getPassword(): string
    {
        return $this->password;
    }




}